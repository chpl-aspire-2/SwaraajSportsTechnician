<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".dashboard.DashBoardActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBar"
        android:elevation="0dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_50sdp"
            android:background="@drawable/bg_home"
            android:contentInsetStart="0dp"
            android:contentInsetLeft="0dp"
            android:contentInsetEnd="0dp"
            android:contentInsetRight="0dp"
            app:contentInsetEnd="0dp"
            app:contentInsetLeft="0dp"
            app:contentInsetRight="0dp"
            app:contentInsetStart="0dp"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/cir_profile"
                    android:elevation="@dimen/_8sdp"
                    android:layout_width="@dimen/_36sdp"
                    android:layout_height="@dimen/_36sdp"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/_8sdp"
                    android:src="@drawable/ic_baseline_settings_24"
                    app:civ_border_color="@color/white"
                    app:civ_border_width="@dimen/_1sdp"
                    app:tint="@color/white" />

                <TextView
                    android:id="@+id/tv_title"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/_8sdp"
                    android:layout_toEndOf="@id/cir_profile"
                    android:ellipsize="end"
                    android:gravity="center_vertical"
                    android:text="Username"
                    android:textAllCaps="false"
                    android:textColor="@color/text_color_white"
                    android:textSize="@dimen/_16ssp"
                    android:textStyle="bold" />

                <RelativeLayout
                    android:id="@+id/rel_noti"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_toEndOf="@id/tv_title">

                    <ImageView
                        android:id="@+id/imgNoti"
                        android:layout_width="@dimen/_25sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/_10sdp"
                        app:srcCompat="@drawable/ic_baseline_notifications_active_24"
                        app:tint="@color/text_color_white" />

                    <TextView
                        android:id="@+id/tv_noti_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_alignParentEnd="true"
                        android:layout_margin="@dimen/_4sdp"
                        android:background="@drawable/circel_red"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/text_color_white"
                        android:textSize="@dimen/_10ssp" />


                </RelativeLayout>

            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>
    </com.google.android.material.appbar.AppBarLayout>


    <androidx.core.widget.NestedScrollView
        android:id="@+id/netstedData"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linLayGetParcels"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="visible">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardMenuGetParcels"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    app:cardBackgroundColor="#12682D7D"
                    app:cardCornerRadius="@dimen/_6sdp"
                    app:cardElevation="0dp"
                    app:cardUseCompatPadding="false">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="@dimen/_10sdp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight=".8"
                                android:orientation="vertical">

                                <RelativeLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <TextView
                                        android:id="@+id/tvGetParcels"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_centerVertical="true"
                                        android:text="@string/complaints"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_14ssp"
                                        android:textStyle="bold" />


                                </RelativeLayout>

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_134sdp"
                                    android:layout_marginTop="@dimen/_8sdp"
                                    android:paddingHorizontal="@dimen/_10sdp"
                                    android:scaleType="fitCenter"
                                    android:src="@drawable/complaint" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.7"
                                android:orientation="vertical">


                                <LinearLayout
                                    android:elevation="@dimen/_4sdp"
                                    android:gravity="center_vertical"
                                    android:id="@+id/linOpen"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:padding="@dimen/_1sdp"
                                    android:background="@drawable/bg_open_new"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="0dp"
                                        android:layout_height="@dimen/_20sdp"
                                        android:layout_gravity="center"
                                        android:layout_weight=".6"
                                        android:gravity="center"
                                        android:padding="@dimen/_4sdp"
                                        android:src="@drawable/ic_close_bg" />

                                    <TextView
                                        android:textSize="@dimen/_10ssp"
                                        android:textColor="@color/black"
                                        android:textStyle="bold"
                                        android:gravity="center_vertical"
                                        android:layout_weight="1"
                                        android:text="OPEN"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"/>

                                    <TextView
                                        android:id="@+id/tv_open_stat"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="center"
                                        android:lines="1"
                                        android:background="@color/close_bg_color"
                                        android:padding="@dimen/_5sdp"
                                        android:text="00"
                                        android:textSize="@dimen/_12ssp"
                                        android:textStyle="bold" />


                                </LinearLayout>

                                <LinearLayout
                                    android:elevation="@dimen/_4sdp"
                                    android:padding="@dimen/_1sdp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:id="@+id/linClose"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_8sdp"
                                    android:background="@drawable/bg_close_new"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="0dp"
                                        android:layout_height="@dimen/_20sdp"
                                        android:layout_gravity="center"
                                        android:layout_weight=".6"
                                        android:gravity="center"
                                        android:padding="@dimen/_4sdp"
                                        android:src="@drawable/ic_completed_all" />


                                    <TextView
                                        android:textSize="@dimen/_10ssp"
                                        android:textColor="@color/black"
                                        android:textStyle="bold"
                                        android:gravity="center_vertical"
                                        android:layout_weight="1"
                                        android:text="CLOSE"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"/>


                                    <TextView
                                        android:id="@+id/tv_close_stat"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:background="@drawable/bg_open_tv"
                                        android:gravity="center"
                                        android:lines="1"
                                        android:padding="@dimen/_5sdp"
                                        android:text="00"
                                        android:textSize="@dimen/_12ssp"
                                        android:textStyle="bold" />


                                </LinearLayout>

                                <LinearLayout
                                    android:padding="@dimen/_1sdp"
                                    android:elevation="@dimen/_4sdp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:id="@+id/linReopen"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_8sdp"
                                    android:background="@drawable/bg_reopen_new"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="0dp"
                                        android:layout_height="@dimen/_20sdp"
                                        android:layout_gravity="center"
                                        android:layout_weight=".6"
                                        android:gravity="center"
                                        android:padding="@dimen/_4sdp"
                                        android:src="@drawable/ic_open_new" />

                                    <TextView
                                        android:textSize="@dimen/_10ssp"
                                        android:textColor="@color/black"
                                        android:textStyle="bold"
                                        android:gravity="center_vertical"
                                        android:layout_weight="1"
                                        android:text="REOPEN"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"/>


                                    <TextView
                                        android:id="@+id/tv_reopen_stat"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:background="#6DA3E7F1"
                                        android:gravity="center"
                                        android:lines="1"
                                        android:padding="@dimen/_5sdp"
                                        android:text="00"
                                        android:textSize="@dimen/_12ssp"
                                        android:textStyle="bold" />


                                </LinearLayout>

                                <LinearLayout
                                    android:padding="@dimen/_1sdp"
                                    android:elevation="@dimen/_4sdp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:id="@+id/linInprogress"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_8sdp"
                                    android:background="@drawable/bg_wip_new"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="0dp"
                                        android:layout_height="@dimen/_20sdp"
                                        android:layout_gravity="center"
                                        android:layout_weight="0.6"
                                        android:gravity="center"
                                        android:padding="@dimen/_4sdp"
                                        android:src="@drawable/ic_wip_bg" />

                                    <TextView
                                        android:textSize="@dimen/_10ssp"
                                        android:textColor="@color/black"
                                        android:textStyle="bold"
                                        android:gravity="center_vertical"
                                        android:layout_weight="1"
                                        android:text="WIP"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"/>


                                    <TextView
                                        android:id="@+id/iv_inprogress_stat"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:background="@color/wip_bg_color"
                                        android:gravity="center"
                                        android:lines="1"
                                        android:padding="@dimen/_5sdp"
                                        android:text="00"
                                        android:textSize="@dimen/_12ssp"
                                        android:textStyle="bold" />


                                </LinearLayout>

                                <LinearLayout
                                    android:padding="@dimen/_1sdp"
                                    android:elevation="@dimen/_4sdp"
                                    android:gravity="center_vertical"
                                    android:layout_gravity="center_vertical"
                                    android:id="@+id/linHold"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_8sdp"
                                    android:background="@drawable/bg_reopen_new"
                                    android:orientation="horizontal">

                                    <ImageView
                                        android:layout_width="0dp"
                                        android:layout_height="@dimen/_20sdp"
                                        android:layout_gravity="center"
                                        android:layout_weight=".6"
                                        android:gravity="center"
                                        android:padding="@dimen/_4sdp"
                                        android:src="@drawable/ic_open_new" />

                                    <TextView
                                        android:textSize="@dimen/_10ssp"
                                        android:textColor="@color/black"
                                        android:textStyle="bold"
                                        android:gravity="center_vertical"
                                        android:layout_weight="1"
                                        android:text="HOLD"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"/>


                                    <TextView
                                        android:id="@+id/tv_hold_stat"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:background="#6DA3E7F1"
                                        android:gravity="center"
                                        android:lines="1"
                                        android:padding="@dimen/_5sdp"
                                        android:text="00"
                                        android:textSize="@dimen/_12ssp"
                                        android:textStyle="bold" />


                                </LinearLayout>


                            </LinearLayout>

                        </LinearLayout>


                        <LinearLayout
                            android:id="@+id/linDataCurrent"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:id="@+id/tv_title_ipc"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="@string/currentassignlist"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_14ssp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <TextView
                                android:gravity="right"
                                android:id="@+id/tv_page_com"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_10ssp"
                                android:textStyle="normal" />


                            <TextView
                                android:id="@+id/tv_title_no"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/_90sdp"
                                android:layout_margin="@dimen/_8sdp"
                                android:gravity="center"
                                android:text="No Data"
                                android:textColor="@color/grey_60"
                                android:textSize="@dimen/_14ssp"
                                android:visibility="gone" />

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/recy_data"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:visibility="gone" />

                            <ProgressBar
                                android:id="@+id/ps_bar2"
                                android:layout_width="wrap_content"
                                android:layout_height="@dimen/_90sdp"
                                android:layout_gravity="center"
                                android:visibility="gone" />

                        </LinearLayout>


                    </LinearLayout>
                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="@dimen/_10sdp"
                android:id="@+id/linLayAsset"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="visible">

                <androidx.cardview.widget.CardView
                    app:cardBackgroundColor="#12682D7D"
                    app:cardElevation="0dp"
                    android:id="@+id/cardMenuAsset"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="@dimen/_6sdp"
                    app:cardUseCompatPadding="false">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="@dimen/_10sdp">

                        <TextView
                            android:id="@+id/tvAssetTitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/assets"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14ssp"
                            app:fontName="OpenSans_Bold" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4sdp"
                            android:orientation="horizontal">


                            <ImageView
                                android:layout_width="0dp"
                                android:layout_height="@dimen/_144sdp"
                                android:layout_centerHorizontal="true"
                                android:layout_weight=".6"
                                android:src="@drawable/assest_new" />


                            <LinearLayout
                                android:layout_marginStart="@dimen/_6sdp"
                                android:paddingBottom="@dimen/_6sdp"
                                android:orientation="vertical"
                                android:id="@+id/relViewAll"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="1">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_corner_assets_all"
                                    android:orientation="horizontal"
                                    android:elevation="@dimen/_2sdp">

                                    <ImageView
                                        android:layout_width="@dimen/_30sdp"
                                        android:layout_height="@dimen/_30sdp"
                                        android:src="@drawable/ic_assets_all"
                                        android:padding="@dimen/_5sdp"/>
                                    <TextView
                                        android:id="@+id/tvViewAll"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/_30sdp"
                                        android:gravity="center|start"
                                        android:text="My Assets"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12sdp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_marginTop="@dimen/_6sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_corner_maintanace_all"
                                    android:orientation="horizontal"
                                    android:elevation="@dimen/_2sdp">

                                    <ImageView
                                        android:layout_width="@dimen/_30sdp"
                                        android:layout_height="@dimen/_30sdp"
                                        android:src="@drawable/ic_maintance_all"
                                        android:padding="@dimen/_5sdp"/>
                                    <TextView
                                        android:id="@+id/tvMainAll"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/_30sdp"
                                        android:gravity="center|start"
                                        android:text="Upcoming Maintenance"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12sdp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_marginTop="@dimen/_6sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_corner_completed_all"
                                    android:orientation="horizontal"
                                    android:elevation="@dimen/_2sdp">

                                    <ImageView
                                        android:layout_width="@dimen/_30sdp"
                                        android:layout_height="@dimen/_30sdp"
                                        android:src="@drawable/ic_completed_all"
                                        android:padding="@dimen/_5sdp"/>
                                    <TextView
                                        android:id="@+id/tvComAll"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/_30sdp"
                                        android:gravity="center|start"
                                        android:text="Completed Maintenance"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12sdp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_marginTop="@dimen/_6sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/bg_corner_missed_all"
                                    android:orientation="horizontal"
                                    android:elevation="@dimen/_2sdp">

                                    <ImageView
                                        android:layout_width="@dimen/_30sdp"
                                        android:layout_height="@dimen/_30sdp"
                                        android:src="@drawable/ic_missed_all"
                                        android:padding="@dimen/_5sdp"/>
                                    <TextView
                                        android:id="@+id/tvPenAll"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/_30sdp"
                                        android:gravity="center|start"
                                        android:text="Missed Maintenance"
                                        android:textColor="@color/black"
                                        android:textSize="@dimen/_12sdp" />
                                </LinearLayout>


                            </LinearLayout>


                        </LinearLayout>


                        <TextView
                            android:id="@+id/tv_title_today_assets"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_8sdp"
                            android:text="@string/todaysmaintenance"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14ssp"
                            android:textStyle="bold" />

                        <TextView
                            android:id="@+id/tv_assets_count"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="0/0"
                            android:gravity="right"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_10ssp"
                            android:textStyle="normal" />

                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/recyAssetsToday"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            tools:itemCount="1"
                            tools:listitem="@layout/today_assets_maintenance" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/linLayResident"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardMenuResident"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/_4sdp"
                    app:cardCornerRadius="@dimen/_6sdp"
                    app:cardElevation="@dimen/_5sdp"
                    app:cardUseCompatPadding="false">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:padding="@dimen/_10sdp">


                        <TextView
                            android:id="@+id/tvAttendanceTitle"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/attendance"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_14ssp"
                            app:fontName="OpenSans_Bold" />

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_4sdp"
                            android:orientation="horizontal"
                            >


                            <ImageView
                                android:layout_width="0dp"
                                android:layout_height="@dimen/_144sdp"
                                android:layout_centerHorizontal="true"
                                android:layout_weight="0.8"
                                android:src="@drawable/attendance_new" />


                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_weight="1"
                                android:layout_marginStart="@dimen/_5sdp"
                                android:elevation="8dp">

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <ImageView
                                        android:src="@drawable/ic_baseline_list_alt_24"
                                        android:layout_width="@dimen/_34sdp"
                                        android:layout_height="@dimen/_34sdp"
                                        app:tint="@color/primary_icon_color_transparent_bg" />

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content">

                                        <TextView
                                            android:id="@+id/tvWeek"
                                            android:textColor="@color/grey_90"
                                            android:textSize="@dimen/_12ssp"
                                            android:textStyle="normal"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"/>

                                        <TextView
                                            android:text="Weekly"
                                            android:textColor="@color/black"
                                            android:textSize="@dimen/_12ssp"
                                            android:textStyle="bold"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"/>


                                    </LinearLayout>

                                </LinearLayout>

                                <LinearLayout
                                    android:orientation="horizontal"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <ImageView
                                        android:src="@drawable/ic_baseline_calendar_month_24"
                                        android:layout_width="@dimen/_34sdp"
                                        android:layout_height="@dimen/_34sdp"
                                        app:tint="@color/primary_icon_color_transparent_bg" />

                                    <LinearLayout
                                        android:orientation="vertical"
                                        android:layout_width="match_parent"
